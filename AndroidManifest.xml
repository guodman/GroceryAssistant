<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.guodman.groceryAssistant"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".Main"
                  android:label="Grocery Assistant">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GroceryList"
                  android:label="Grocery List">
        </activity>
        <activity android:name=".FoodList"
                  android:label="Food List">
        </activity>
        <activity android:name=".CreateItem"
                  android:label="Create Item">
        </activity>
        <activity android:name=".EditItem"
                  android:label="Edit Item">
        </activity>
    </application>
</manifest>
